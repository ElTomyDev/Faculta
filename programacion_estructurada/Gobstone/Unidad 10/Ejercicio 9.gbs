program{
    fecha1 := Fechas(
        dia <- 20,
        mes <- Mayo,
        año <- 2000
        )
    fecha2 := Fechas(
        dia <- 1,
        mes <- Enero,
        año <- 2024
        )
        
    return(esMásAntigua_Que_(fecha1, fecha2))
}

# Ejercicio 9
type Fechas is record{
    field dia //Numero
    field mes //Mes
    field año //Numero
}

type Mes is variant{
    case Enero {}
    case Febrero {}
    case Marzo {}
    case Abril {}
    case Mayo {}
    case Junio {}
    case Julio {}
    case Agosto {}
    case Septiembre {}
    case Octubre {}
    case Noviembre {}
    case Diciembre {}
}

# Punto (A)
function esDiaDeÑoquis_(miFecha){
    return(dia(miFecha) == 29)
}

# Punto (B)
function esDelPrimerSemestre_(miFecha){
    return(numeroDelMes_(mes(miFecha)) <= numeroDelMes_(Junio))

}

function numeroDelMes_(mes){
    return(
        choose
            1 when (mes == Enero)
            2 when (mes == Febrero)
            3 when (mes == Marzo)
            4 when (mes == Abril)
            5 when (mes == Mayo)
            6 when (mes == Junio)
            7 when (mes == Julio)
            8 when (mes == Agosto)
            9 when (mes == Septiembre)
            10 when (mes == Octubre)
            11 when (mes == Noviembre)
            12 otherwise
        )
}

# Punto (C)
function esFechaDeAñoBisiesto_(miFecha){
    
    return(
        año(miFecha) mod 4 == 0     &&
        año(miFecha) mod 100 == 0   &&
        año(miFecha) mod 400 == 0
    )
}

# Punto (D)
function primerDiaDelInviernoDe_(miFecha){
    return(
        choose
            20 when (esFechaDeAñoBisiesto_(miFecha))
            21 otherwise
        )
}

# Punto (E)
function ultimoDiaDelInviernoDe_(miFecha){
    return(
        choose
            22 when (esFechaDeAñoBisiesto_(miFecha))
            23 otherwise
        )
}

# Punto (F)
